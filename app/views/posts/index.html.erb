<h1>This Index will list all the posts</h1> 

<%= @posts %>

<table>
    <thead>
        <tr>
          <th>ID</th>
          <th>title</th>
          <th>Published</th>
      </tr>
  </thead>
  
  <tbody>
    <% @posts.each do |whatever| %>
      <tr>
        <td><%= whatever.id %></td>
        <td><a href="/posts/<%= whatever.id%>"><%= whatever.title %></a></td>
        <td><%= whatever.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>